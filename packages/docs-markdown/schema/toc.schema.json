{
	"title": "TOC",
	"$schema": "https://dotnet.github.io/docfx/schemas/v1.0/schema.json#",
	"version": "1.0.0",
	"description": "The schema for a TOC object.",
	"id": "https://github.com/dotnet/docfx/test/schemas/toc.schema.json",
	"type": "object",
	"additionalProperties": false,
	"required": ["items"],
	"properties": {
		"items": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/tocNode"
			}
		},
		"metadata": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"universal_ref_toc": {
					"type": "string"
				},
				"universal_conceptual_toc": {
					"type": "string"
				},
				"nodes_to_expand": {
					"type": "string"
				},
				"pdf_url_template": {
					"type": "string"
				},
				"pdf_absolute_path": {
					"type": "string"
				},
				"pdf_name": {
					"type": "string"
				}
			}
		}
	},
	"definitions": {
		"tocNode": {
			"properties": {
				"displayName": {
					"type": "string",
					"tags": ["localizable"]
				},
				"expanded": {
					"type": "boolean"
				},
				"href": {
					"type": "string"
				},
				"homepage": {
					"type": "string"
				},
				"items": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/tocNode"
					}
				},
				"name": {
					"type": "string",
					"tags": ["localizable"]
				},
				"preserveContext": {
					"type": "boolean"
				},
				"tocHref": {
					"type": "string"
				},
				"topicHref": {
					"type": "string"
				},
				"topicUid": {
					"type": "string"
				},
				"type": {
					"type": "string"
				}
			},
			"additionalProperties": false,
			"required": ["name"],
			"type": "object"
		}
	}
}
