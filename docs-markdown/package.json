    "keybindings": [
      {
        "command": "markdownQuickPick",
        "key": "Alt+M",
        "mac": "Alt+M",
        "when": "editorTextFocus"
      },
      {
        "command": "insertNestedList",
        "key": "tab",
        "mac": "tab",
        "when": "editorTextFocus && editorLangId == markdown && !suggestWidgetVisible && !vim.active"
      },
      {
        "command": "formatBold",
        "key": "Alt+D B",
        "mac": "Alt+D B",
        "when": "editorTextFocus && editorLangId == 'markdown' && !vim.active"
      },
      {
        "command": "formatItalic",
        "key": "Alt+D I",
        "mac": "Alt+D I",
        "when": "editorTextFocus && editorLangId == 'markdown' && !vim.active"
      },
      {
        "command": "insertURL",
        "key": "Alt+D K",
        "mac": "Alt+D K",
        "when": "editorTextFocus && editorLangId == 'markdown' && !vim.active"
      },
      {
        "command": "insertLink",
        "key": "Alt+D L",
        "mac": "Alt+D L",
        "when": "editorTextFocus && editorLangId == 'markdown' && !vim.active"
      },
      {
        "command": "previewTopic",
        "key": "Alt+D P",
        "mac": "Alt+D P",
        "when": "editorTextFocus && editorLangId == 'markdown' && !vim.active"
      }
    ],
    "yamlValidation": [
      {
        "fileMatch": "toc.yml",
        "url": "./schema/toc.schema.json"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test-core": "node ./node_modules/vscode/bin/test",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "diff": "^4.0.1",
    "file-exists": "^5.0.1",
    "fs-exists-sync": "^0.1.0",
    "glob": "^7.1.6",
    "graceful-fs": "^4.1.15",
    "js-yaml": "^3.13.1",
    "lodash.merge": "^4.6.2",
    "markdownlint": "^0.11.0",
    "matcher": "^1.0.0",
    "node-dir": "^0.1.17",
    "recursive-readdir": "^2.2.2",
    "typescript-collections": "^1.2.3",
    "vscode-extension-telemetry": "^0.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/glob": "^5.0.35",
    "@types/graceful-fs": "^2.0.30",
    "@types/jest": "24.0.12",
    "@types/lodash.merge": "^4.6.3",
    "@types/node": "^7.10.5",
    "@types/node-dir": "0.0.32",
    "@types/recursive-readdir": "^2.2.0",
    "@types/yamljs": "^0.2.30",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "octicons": "^7.3.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.9.1",
    "typescript": "3.4.5",
    "vscode": "^1.1.33"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/test"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true
  },
  "jest-junit": {
    "output": "test-results-jest.xml",
    "usePathForSuiteName": "true"
  },
