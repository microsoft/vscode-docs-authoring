{
	"name": "docs-yaml",
	"displayName": "docs-yaml",
	"description": "YAML schema validation and auto-completion for docs.microsoft.com authoring",
	"aiKey": "0a0e5961-85c2-451a-bce8-6a54e37c93be",
	"version": "0.2.5",
	"publisher": "docsmsft",
	"engines": {
		"vscode": "^1.43.0"
	},
	"license": "MIT",
	"categories": [
		"Azure",
		"Snippets",
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"yaml",
		"YamlMime",
		"azure",
		"docs"
	],
	"icon": "images/docs-logo-ms.png",
	"bugs": {
		"url": "https://github.com/vscode-docs-authoring/docs-yaml/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-docs-authoring.git"
	},
	"activationEvents": [
		"onLanguage:yaml"
	],
	"main": "./dist/extension",
	"contributes": {},
	"extensionDependencies": [
		"redhat.vscode-yaml"
	],
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"prettier": "prettier --write \"**/*.{ts,js,json,yml}\"",
		"prettier-check": "prettier --check \"**/*.{ts,js,json,yml}\" --loglevel debug",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"pretest": "npm run compile",
		"test": "node ./out/src/test/runTest.js",
		"lint": "eslint . --ext .ts",
		"precommit": "pretty-quick --staged --pattern \"**/*.{ts,js,json,yml}\"",
		"pre-push": "cd ../.. && yarn run compile && yarn run lint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged --pattern \"**/*.{ts,js,json,yml}\"",
			"pre-push": "yarn run pre-push"
		}
	},
	"dependencies": {
		"copy-webpack-plugin": "^5.1.1",
		"fuse.js": "3.4.5",
		"js-yaml": "^3.13.1",
		"vscode-extension-telemetry": "^0.1.1",
		"web-request": "^1.0.7"
	},
	"devDependencies": {
		"@types/chai": "^4.2.11",
		"@types/chai-spies": "^1.0.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^6.0.40",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/eslint-plugin-tslint": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"chai": "^4.2.0",
		"chai-spies": "^1.0.0",
		"eslint": "^7.0.0",
		"eslint-import-resolver-webpack": "^0.12.1",
		"eslint-plugin-import": "^2.20.2",
		"husky": "^4.2.5",
		"mocha": "^7.1.1",
		"mocha-junit-reporter": "^1.23.3",
		"nyc": "^15.0.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"terser-webpack-plugin": "^2.3.5",
		"ts-loader": "^6.2.2",
		"ts-node": "^8.8.1",
		"tslint": "^6.0.0",
		"typescript": "^3.9.3",
		"vscode": "^1.1.33",
		"vscode-test": "^1.3.0",
		"webpack": "^4.42.1",
		"webpack-bundle-analyzer": "^3.6.1",
		"webpack-cli": "^3.3.11"
	}
}
