{
	"cSpell.words": ["Synchrounous"],
	"editor.codeActionsOnSave": {
		"source.eslint.fixAll": true
	},
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnPaste": false,
	"editor.tabSize": 2,
	"files.exclude": {
		"out": false
	},
	"search.exclude": {
		"out": true
	},
	"typescript.tsc.autoDetect": "off"
}
